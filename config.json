{
    "repos": [
        {
           "repo": "black2c7/Emby-icons",
           "type": "github",
           "dir": [
                "Icons",
                "Icons2",
                "Icons3"
           ]
        },
        {
            "repo": "Softlyx/Fileball",
            "type": "github",
            "dir": [
                "FANG",
                "YUAN"
            ]
        }
    ],
    "proxy_rules": {
        "ghproxy": {
            "proxy": "https://ghproxy.com/",
            "format": "{proxy}https://raw.githubusercontent.com/{owner}/{repo}/{branch}/{file}"
        },
        "jsdelivr": {
            "proxy": "https://cdn.jsdelivr.net/gh/",
            "format": "{proxy}{owner}/{repo}@{branch}/{file}"
        },
        "ghproxy.net": {
            "proxy": "https://ghproxy.net/",
            "format": "{proxy}https://raw.githubusercontent.com/{owner}/{repo}/{branch}/{file}"
        }
    }
}